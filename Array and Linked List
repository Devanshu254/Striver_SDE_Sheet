Q1. Rotate a linked list.
Solution -> 
class Solution {
public:
    ListNode* rotateRight(ListNode* head, int k) {
        if(!head || !head->next || k==0) return head;
        int count = 1;
        ListNode* curr = head;
        while(curr->next != NULL){
            curr = curr->next;
            count++;
        }
        curr->next = head;
        k = k%count;
        k = count - k;
        while(k--) curr = curr->next;
        head = curr->next;
        curr->next = NULL;
        return head; 
    }
};
